{
  "version": 2,
  "buildCommand": "nest build",
  "builds": [
    {
      "src": "dist/**/*.*",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "stc/main.ts",
      "methods": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
