{
  "version": 2,
  "buildCommand": "nest build",
  "builds": [
    {
      "src": "dist/**/*.*",
      "use": "@vercel/node"
    },
    {
      "src": "client/**/*.*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
